<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="create_run_jar" name="Deploy TeaseLib">
    <!--this file was created by Eclipse Runnable JAR Export Wizard-->
    <!--ANT 1.7 is required                                        -->
    <target name="create_run_jar">
        <jar destfile="lib/TeaseLib.jar">
            <manifest>
                <attribute name="Class-Path" value="lib/jinput.jar lib/jump3er-1.03.jar lib/webcam-capture-0.3.10.jar"/>
            </manifest>
            <fileset dir="bin"/>
        </jar>
    </target>
    <target name="create_tests_jar">
        <jar destfile="lib/TeaseLib_tests.jar">
            <manifest>
                <attribute name="Class-Path" value="lib/TeaseLib.jar"/>
            </manifest>
            <fileset dir="bin.test"/>
        </jar>
    </target>
</project>
