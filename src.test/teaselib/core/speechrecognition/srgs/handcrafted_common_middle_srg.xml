<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<grammar xmlns="http://www.w3.org/2001/06/grammar" root="Main"
	tag-format="semantics/1.0" version="1.0" xml:lang="en-US">
	<!-- handcrafted_srg.xml: Defines an SRGS grammar for testing
		Node Rule_1_Result is needed to access Rule_2_Choice_1 as its child
	 
	<example>  Yes Miss I've spurted off </example>
	<example>  No Miss I didn't spurt off </example>
	 -->
	
	<rule id="Main" scope="public">
		<one-of>
			<item>
				<ruleref uri="#r_0_0_1" />
				<ruleref uri="#r_1_0,1_2" />
				<ruleref uri="#r_2_0_4" />
			</item>
			<item>
				<ruleref uri="#r_0_1_3" />
				<ruleref uri="#r_1_0,1_2" />
				<ruleref uri="#r_2_1_5" />
			</item>
		</one-of>
		<ruleref uri="#r_3_0,1_6" />
	</rule>

	<rule id="r_0_0_1" scope="private">
		Yes
	</rule>

	<rule id="r_0_1_3" scope="private">
		No
	</rule>

	<rule id="r_1_0,1_2" scope="private">
		Miss
	</rule>

	<rule id="r_2_0_4" scope="private">
		I've spurted
	</rule>

	<rule id="r_2_1_5" scope="private">
		I didn't spurt
	</rule>

	<rule id="r_3_0,1_6" scope="private">
		off
	</rule>

</grammar>